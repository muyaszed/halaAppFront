{
  "name": "halaAppFront",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write '**/*.js'",
    "format-code": "npm run prettier && npm run lint:fix",
    "postinstall": "./node_modules/react-native-maps/enable-google-maps 'ios/Frameworks/GoogleMaps/**'"
  },
  "dependencies": {
    "acorn-jsx": "^5.0.1",
    "cross-fetch": "^3.0.1",
    "handlebars": "^4.0.14",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.0",
    "jwt-lite": "^1.1.2",
    "prop-types": "^15.6.2",
    "react": "16.6.3",
    "react-native": "^0.59.9",
    "react-native-config": "^0.11.7",
    "react-native-dotenv": "^0.2.0",
    "react-native-fbsdk": "^0.10.1",
    "react-native-gesture-handler": "^1.0.12",
    "react-native-image-picker": "^0.28.0",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-launch-navigator": "^1.0.5",
    "react-native-maps": "^0.24.2",
    "react-native-open-maps": "^0.3.3",
    "react-native-paper": "^2.16.0",
    "react-native-pure-jwt": "^1.6.0",
    "react-native-vector-icons": "^6.1.0",
    "react-navigation": "^3.0.9",
    "react-navigation-material-bottom-tabs": "^1.0.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "ws": "^3.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "detox": "^12.11.2",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.17.3",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.51.1",
    "prettier": "1.16.4",
    "react-native-testing-library": "^1.5.0",
    "react-test-renderer": "^16.7.0"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "testRegex": "__test__/.*\\.(js|jsx)$"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/halaAppFront.app",
        "build": "xcodebuild -project ios/halaAppFront.xcodeproj -UseNewBuildSystem=NO -scheme halaAppFront -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 8"
      }
    },
    "test-runner": "jest"
  }
}
